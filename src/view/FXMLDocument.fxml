<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="controller.FXMLDocumentController">
    <children>
        <Button fx:id="button" layoutX="126" layoutY="90" onAction="#handleButtonAction" text="Click Me!" />
        <Label fx:id="label" layoutX="126" layoutY="120" minHeight="16" minWidth="69" />
      <Button fx:id="update" layoutX="20.0" layoutY="138.0" onAction="#Update" text="update" />
      <Button fx:id="delete" layoutX="20.0" layoutY="87.0" onAction="#Delete" text="delete" />
      <Button fx:id="create" layoutX="20.0" layoutY="32.0" onAction="#Create" text="create" />
      <Button fx:id="read" layoutX="126.0" layoutY="32.0" onAction="#Read" text="read" />
      <Button fx:id="readWithNameAndCaloris" layoutX="196.0" layoutY="32.0" onAction="#readWithNandC" text="read with name and caloris" />
      <Button fx:id="readWithName" layoutX="225.0" layoutY="87.0" onAction="#readWithName" text="read with name " />
      <Button fx:id="readWithID" layoutX="235.0" layoutY="138.0" onAction="#readWithID" text="read with ID" />
    </children>
</AnchorPane>
